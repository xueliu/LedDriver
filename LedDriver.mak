	$(OUTDIR)/RuntimeErrorStub.o $(OUTDIR)/LedDriver.o \
	$(OUTDIR)/LedDriverTest.o $(OUTDIR)/LedDriverTestRunner.o 
OBJ=$(COMMON_OBJ) $(CFG_OBJ)
ALL_OBJ=$(OUTDIR)/unity_fixture.o $(OUTDIR)/unity.o \
	$(OUTDIR)/RuntimeErrorStub.o $(OUTDIR)/LedDriver.o \
	$(OUTDIR)/LedDriverTest.o $(OUTDIR)/LedDriverTestRunner.o 

