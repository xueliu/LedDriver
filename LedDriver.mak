	$(COMPILE)

$(OUTDIR)/%.o : src/%.c
	$(COMPILE)

$(OUTDIR)/%.o : extras/fixture/src/%.c
	$(COMPILE)

$(OUTDIR)/%.o : util/%.c
	$(COMPILE)

# Build rules
all: $(OUTFILE)

$(OUTFILE): $(OUTDIR)  $(OBJ)
	$(LINK)

$(OUTDIR):
	$(MKDIR) -p "$(OUTDIR)"

# Rebuild this project
rebuild: cleanall all

# Clean this project
clean:
	$(RM) -f $(OUTFILE)
